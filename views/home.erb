<h1>Mst Recnt mumbls</h1>
<% Post.last(10).reverse.each do |post| %>
	<div class="last_ten_post_container">
		<div class="post_username">
			<form method="POST" action="/other">
				<input hidden name="clicked_user" value="<%= post.user.username%>"/>
				<button class="user_click"><%= post.user.username %></button> mumbls...	
			</form>
			<h3><%= post.user.username%> mumbls...</h3>
		</div>
		<div class="post_content">
			<%= post.content %>
		</div>
	</div>
<% end %>